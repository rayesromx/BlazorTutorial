@page "/students"
@using BlazorTutorial.Models
<h3>Students</h3>
<PageTitle>Students</PageTitle>

<div>
    @foreach (var s in GetStudents())
    {
        <div>
            Student
            <p>Name: @s.Name</p>
            <p>ID: @s.StudentId</p>
            <p>Email: @s.Email</p>
        </div>
    }
    
</div>


